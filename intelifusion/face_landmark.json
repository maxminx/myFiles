{
  "log_dir": "/data/aios/out/visiontl/landmark",
  "dataset": {
    "data_root": "/data/aios/data/landmark",
    "train_dir": "train",
    "valid_dir": null,
    "test_dir": null,
    "valid_size": 0.2,
    "test_size": 0.1,
    "num_joints": 68
  },
  "gpus": [
    1
  ],
  "model": {
    "base_network": "resnet18_v2",
